function Search-WinEventLogs {
    [CmdletBinding()]
    param (
        [Parameter(Mandatory = $true, HelpMessage = "Enter the name of a Windows Log.")]
        [ValidateSet("Application","Security","System","Setup")]
        [string]$LogName,
        [Parameter(Mandatory = $false)]
        [int[]]$EventID,
        [Parameter(Mandatory = $false)]
        [datetime]$StartTime,
        [Parameter(Mandatory = $false)]
        [datetime]$EndTime,
        [Parameter(Mandatory = $false)]
        [Int32]$count,
        [Parameter(Mandatory = $false)]
        [Switch]$LogonEvents
        )


$EventLogs = @{
        logname = $Logname
        }

    if ($PSBoundParameters.ContainsKey('EventID')) {
        $EventLogs.Add('ID', $EventID)
        }
    
    if ($PSBoundParameters.ContainsKey('StartTime')) {
        $EventLogs.Add('StartTime', $StartTime)
        $EventLogs.Add('EndTime', $EndTime)
        }
    if ($PSBoundParameters.ContainsKey('Count')) {
        $Limit = $count
        }
    else {
        $Limit = 5
        }

    Get-WinEvent -FilterHashtable $EventLogs | Select-Object -First $Limit
    
}
